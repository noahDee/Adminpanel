<h1>Edit</h1>
<p>Edit your info</p>
<div class="container main">

<%= form_for(@instr, url: edit_instructor_url) do |f|%>

<%= f.label :email%>
<%= f.email_field :email%><br>
<%= f.label :password%>
<%= f.password_field :password%><br>
<%= f.label :password_confirmation%>
<%= f.password_field :password_confirmation%><br>


<%= f.submit "Submit"%>
<% end %>

</div>
